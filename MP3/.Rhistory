#Create a linear progression prediction on petal width values based on petal length
#
irisdata <- iris
#independent/predictor variable, needed for prediction
petalLength <- subset(irisdata, select = Petal.Length)
#dependent/response variable, value to be predicted
petalWidth <- subset(irisdata, select = Petal.Width)
#data containing independent and dependent variable
lengthandwidth <- data.frame(petalLength, petalWidth)
View(lengthandwidth)
#linear model
lngnwidmodel <- lm(unlist(petalWidth) ~ unlist(petalLength), data = lengthandwidth)
summary(lngnwidmodel)
# y = alpha + beta(x)
#alpha(y-intercept) = -0.363076
#beta (slope) = 0.415755
#mathematical model = -0.363076 + 0.415755(Petal Length)
#line of fit
plot(lengthandwidth)
abline(lngnwidmodel, col = "red", lwd = 6)
